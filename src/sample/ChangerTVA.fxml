<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="100.0" prefWidth="300.0" style="-fx-background-color: #2C2C2C; -fx-border-width: 4; -fx-border-color: #9E7F37;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChangerTVAController">
   <children>
      <JFXTextField fx:id="textTVA" labelFloat="true" layoutX="22.0" layoutY="30.0" prefHeight="42.0" prefWidth="242.0" promptText="Nouveaux valeur TVA" style="-fx-background-color: #424242; -fx-text-inner-color: #44BBED; -fx-prompt-text-fill: #44BBED;" unFocusColor="#486a73">
         <font>
            <Font size="19.0" />
