            <Font name="Arial Black" size="16.0" />
         </font>
         <textFill>
            <LinearGradient endX="0.6407766990291262" endY="0.8932038834951457">
               <stops>
                  <Stop color="#1942e4" />
                  <Stop color="#ff5a5a" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </JFXButton>
      <JFXButton fx:id="buttonChanger" layoutX="96.0" layoutY="76.0" onAction="#changerButtonClicked" text="Changer">
